a)\n","column_transformer_X = sklearn.compose.ColumnTransformer(transformers=[\n","    (\"total_bill\", \"passthrough\", [0]),\n","    (\"tip\", \"drop\", [1]),\n","    (\"sex\", \"drop\", [2]),\n","    (\"smoker\", \"drop\", [3]),\n","    (\"day\", \"drop\", [4]),\n","    (\"time\", sklearn.preprocessing.OneHotEncoder(), [5]),\n","    (\"size\", StandardScaler(), [6])\n","]);\n","\n","column_transformer_Y = sklearn.compose.ColumnTransformer(transformers=[\n","    (\"id\", \"drop\", [0]),\n","    (\"total_bill\", \"passthrough\", [1]),\n","    (\"sex\", \"drop\", [2]),\n","    (\"smoker\", \"drop\", [3]),\n","    (\"day\", \"drop\", [4]),\n","    (\"time\", sklearn.preprocessing.OneHotEncoder(), [5]),\n","    (\"size\", StandardScaler(), [6])\n","]);\n","\n","X_transform  = column_transformer_X.fit_transform(X)\n","Y_transform  = column_transformer_Y.fit_transform(Y)\n","\n","\n","# Entrenar el modelo con el dataset de entrenamiento y la variable objetivo\n","#reg = LinearRegression().fit(X_transform, X[\"tip\"])\n","\n","alpha = 4\n","ratio = 0.66\n","reg = ElasticNet(alpha=alpha, l1_ratio=ratio).fit(X_transform, X[\"tip\"])\n","\n","#reg.hist('housing_median_age')\n","\n","\n","scores = cross_val_score(reg, X_transform, X[\"tip\"], cv=5, scoring='neg_mean_squared_error')\n","print(np.mean(scores))\n","\n","\n","\n","# Hacer una predicción de Y\n","\n","pred = reg.predict(Y_transform)\n","print(pred)\n","\n","# Exportar solución a CSV\n","solucion = pd.DataFrame(data = { \"id\": Y[\"id\"], \"tip\": pred })\n","solucion.to_csv(\"solucion.csv\", index=False)\n","solucion"]},{"cell_type":"code","source":["X[\"size\"]"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"z9XDw8AVuJDX","executionInfo":{"status":"ok","timestamp":1664984778695,"user_tz":-120,"elapsed":429,"user":{"displayName":"Alejandro Cortijo","userId":"18335603417479942728"}},"outputId":"01a0c751-2dae-4b3b-e9d4-2a5a1808a653"},"execution_count":null,"outputs":[{"output_type":"execute_resul